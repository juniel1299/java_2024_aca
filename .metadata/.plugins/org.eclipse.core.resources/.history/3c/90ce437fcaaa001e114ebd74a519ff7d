package com.test.java;

public class Ex15_Method {

	public static void main(String[] args) {
		
		//Ex15_Method
		//public static void hello()
		
		//void > 반환타입
		//hello > 메서드명
		// () > 인자리스트
		
		//메서드 인자리스트
		//-파라미터
		hello("아무개");
		hello("홍길동");
		
	}//main
	
	public static void hello(String name) {
		
		System.out.println(name+"님. 안녕하세요.");
	}
	
}//Ex15_Method
